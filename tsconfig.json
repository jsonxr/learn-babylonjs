{
  "compilerOptions": {
    "target": "ES2018",
    "module": "esnext",
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": [
        "babylonjs",
        "babylonjs-gui",
        "babylonjs-materials"
    ]
  }
}
